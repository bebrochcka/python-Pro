a=[]
sus=[]
b=9201848091284891204821094809384092348032049840928434234324242342424424243333423423252534222222223244323422222222222222222243543
n=int(input("Сколько чисел добавить?"))
for i in range(n):
    a.append(int(input("Какие числа добавить?")))
    if(a[i]<=b):
        b=a
    if(a[i]/0):
        print("На 0 делить нельзя")
    sus.append()
print(sus) 
