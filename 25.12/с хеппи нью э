a=input("Введите страну")
if(a=="Россия"):
    print("Вас поздравляет Дед мороз")
    print("С новым годом!")
if(a=="Италия"):
    print("Вас поздравляет Баббо Натале")
    print("Felice anno nuovo!")
if(a=="Финляндия"):
    print("Вас поздравляет Йоулупукки")
    print("Hyvää Uutta Vuotta!")
if(a=="Норвегия"):
    print("Вас поздравляет Юлебукк")
    print("Godt Nytt År!")
if(a=="Великобритания"):
    print("Вас поздравляет ФазерКристмас")
    print("Happy new year!")
if(a=="Албания"):
    print("Вас поздравляет Бабадимпи")
    print("Gëzuar Vitin E Ri!")
if(a=="Греция"):
    print("Вас поздравляет Агиос Василис")
    print("Ευτυχισμένο Το Νέο Έτοςм!")
if(a=="Татарстан"):
    print("Вас поздравляет Кышбабай")
    print("Яңа ел белән!")
if(a=="Франция"):
    print("Вас поздравляет Пер Ноель")
    print("Bonne année!")
if(a=="Япония"):
    print("Вас поздравляет Сегацу-сан")
    print("新年おめでとうございます。!")
